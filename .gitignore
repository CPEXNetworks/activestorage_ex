# The directory Mix will write compiled artifacts to.
/_build/

# If you run "mix test --cover", coverage assets end up here.
/cover/

# The directory Mix downloads your dependencies sources to.
/deps/

# Where 3rd-party dependencies like ExDoc output generated docs.
/doc/

# Ignore .fetch files in case you like to edit your project deps locally.
/.fetch

# If the VM crashes, it generates a dump, let's ignore it too.
erl_crash.dump

# Also ignore archive artifacts (built via "mix archive.build").
*.ez

# Ignore package tarball (built via "mix hex.build").
activestorage_ex-*.tar

# Ignore Elixir language server
.elixir_ls

# Ignore appropriate Rails files
# Ignore bundler config.
/test/activestorage_ex_rails/.bundle

# Ignore the default SQLite database.
/test/activestorage_ex_rails/db/*.sqlite3
/test/activestorage_ex_rails/db/*.sqlite3-journal

# Ignore all logfiles and tempfiles.
/test/activestorage_ex_rails/log/*
/test/activestorage_ex_rails/tmp/*
!/test/activestorage_ex_rails/log/.keep
!/test/activestorage_ex_rails/tmp/.keep

# Ignore uploaded files in development
/test/activestorage_ex_rails/storage/*
!/test/activestorage_ex_rails/storage/.keep

/test/activestorage_ex_rails/node_modules
/test/activestorage_ex_rails/yarn-error.log

/test/activestorage_ex_rails/public/assets
/test/activestorage_ex_rails.byebug_history

# Ignore master key for decrypting credentials and more.
/test/activestorage_ex_rails/config/master.key
